{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Defcomm",
  "version": "0.1.0",
  "identifier": "com.tauri-app.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Defcomm App",
        "width": 1000,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/30x30/1.png",
      "icons/30x30/2.png",
      "icons/128x128/1.png",
      "icons/128x128/2.png",
      "icons/128x128 ICNS/1.icns",
      "icons/128x128 ICNS/2.icns",
      "icons/128x128 ICO/1.ico",
      "icons/128x128 ICO/2.ico"
    ]
  }
}
